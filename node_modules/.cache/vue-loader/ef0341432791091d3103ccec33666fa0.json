         <th class=\"ps-1\" colspan=\"4\">\n                <p\n                  class=\"\n                    mb-0\n                    text-body text-capitalize text-md\n                    font-weight-light\n                  \"\n                >\n                  Activity\n                </p>\n              </th>\n              <th class=\"text-center\">\n                <p\n                  class=\"\n                    mb-0\n                    text-body text-capitalize text-md\n                    font-weight-light\n                  \"\n                >\n                  Email\n                </p>\n              </th>\n              <th class=\"text-center\">\n                <p\n                  class=\"\n                    mb-0\n                    text-body text-capitalize text-md\n                    font-weight-light\n                  \"\n                >\n                  Push\n                </p>\n              </th>\n              <th class=\"text-center\">\n                <p\n                  class=\"\n                    mb-0\n                    text-body text-capitalize text-md\n                    font-weight-light\n                  \"\n                >\n                  SMS\n                </p>\n              </th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"(item, i) in notifications\" :key=\"item.title\">\n              <td\n                class=\"text-sm text-secondary mb-0 py-3 ps-1\"\n                :class=\"{ 'border-bottom': i != notifications.length - 1 }\"\n                colspan=\"4\"\n              >\n                <span class=\"text-dark d-block text-sm\" :v-if=\"item.title\">{{\n                  item.title\n                }}</span>\n                <span\n                  class=\"text-xs font-weight-light\"\n                  :v-if=\"item.description\"\n                  >{{ item.description }}</span\n                >\n                <p class=\"text-sm font-weight-light mb-0\" :v-if=\"item.text\">\n                  {{ item.text }}\n                </p>\n              </td>\n              <td>\n                <v-switch\n                  :ripple=\"false\"\n                  class=\"mt-0 pt-0 ms-8 switch\"\n                  v-model=\"item.switch1\"\n                  hide-details\n                ></v-switch>\n              </td>\n              <td>\n                <v-switch\n                  :ripple=\"false\"\n                  class=\"mt-0 pt-0 ms-5 switch\"\n                  v-model=\"item.switch2\"\n                  hide-details\n                ></v-switch>\n              </td>\n              <td>\n                <v-switch\n                  :ripple=\"false\"\n                  class=\"mt-0 pt-0 ms-5 switch\"\n                  v-model=\"item.switch3\"\n                  hide-details\n                ></v-switch>\n              </td>\n            </tr>\n          </tbody>\n        </template>\n      </v-simple-table>\n    </div>\n  </v-card>\n</template>\n<script>\nexport default {\n  name: \"notifications\",\n  data() {\n    return {\n      notifications: [\n        {\n          title: \"Mentions\",\n          description: \"Notify when another user mentions you in a comment\",\n          switch1: true,\n          switch2: false,\n          switch3: false,\n        },\n        {\n          title: \"Comments\",\n          description: \"Notify when another user comments your item\",\n          switch1: true,\n          switch2: true,\n          switch3: false,\n        },\n        {\n          title: \"Follows\",\n          description: \"Notify when another user follows you\",\n          switch1: false,\n          switch2: true,\n          switch3: false,\n        },\n        {\n          text: \"Log in from a new device\",\n          switch1: true,\n          switch2: true,\n          switch3: true,\n        },\n      ],\n    };\n  },\n};\n</script>\n"]}]}